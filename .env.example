# App
APP_ENV=development
APP_HOST=0.0.0.0
APP_PORT=8000
APP_HOST_PORT=8000
DOMAIN=example.com
COMPOSE_PROJECT_NAME=sms-chatbot
SECRET_KEY=change-me-in-production
BUSINESS_NAME="Acme Services"
ADMIN_API_KEY=change-this-admin-key
ADMIN_USERNAME=admin
ADMIN_PASSWORD=change-admin-password
SUPPORT_PHONE_NUMBER=+15551234567

# Database
DATABASE_URL=postgresql+asyncpg://postgres:postgres@db:5432/sms_chatbot
DB_POOL_SIZE=5
DB_MAX_OVERFLOW=10
POSTGRES_HOST_PORT=5433

# Redis
REDIS_URL=redis://redis:6379/0
REDIS_HOST_PORT=6380

# Twilio
TWILIO_ACCOUNT_SID=ACxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx
TWILIO_AUTH_TOKEN=your-twilio-auth-token
TWILIO_API_ACCOUNT_SID=SKxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx
TWILIO_API_AUTH_TOKEN=your-twilio-api-auth-token
TWILIO_PHONE_NUMBER=+15551234567
TWILIO_STATUS_CALLBACK_URL=https://yourdomain.com/webhooks/sms/status
TWILIO_MAX_RETRIES=3
TWILIO_MAX_SENDS_PER_SECOND=1

# OpenRouter (AI)
OPENROUTER_API_KEY=sk-or-v1-your-openrouter-key
VERCEL_AI_GATEWAY_API_KEY=
AI_PROVIDER=openrouter
AI_BASE_URL=
AI_MODEL=qwen/qwen3.5-397b-a17b
AI_MAX_TOKENS=300
ADMIN_SESSION_SECRET=change-admin-session-secret
ADMIN_SESSION_MAX_AGE_SECONDS=43200

# Campaign Defaults
DEFAULT_QUIET_HOURS_START=21:00
DEFAULT_QUIET_HOURS_END=09:00

# Dev it's an api key for ngrok
NGROK_AUTHTOKEN=your-ngrok-authtoken
NGROK_API_KEY=your-ngrok-api-key
NGROK_DASHBOARD_PORT=4041
